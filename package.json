      "@angular/material": ">=18.0.0",
      "@angular/cdk": ">=18.0.0"
    },
    "@angular-material-components/moment-adapter": {
      "@angular/common": ">=18.0.0",
      "@angular/core": ">=18.0.0"
    }
  }
